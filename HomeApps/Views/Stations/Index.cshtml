@model IEnumerable<HomeApps.Station>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_GasLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.Deleted)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.CreateModifyLog.CreateModifyID)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>
            @Html.DisplayFor(modelItem => item.Deleted)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.CreateModifyLog.CreateModifyID)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StationID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StationID })
        </td>
    </tr>
}

</table>
